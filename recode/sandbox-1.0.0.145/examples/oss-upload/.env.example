# Sandbox URL (default: http://localhost:8080)
SANDBOX_BASE_URL=http://localhost:8080

# ===== TOS (Volcano Engine TOS) Configuration (for main.py) =====
# Required for Volcano Engine TOS upload
TOS_BUCKET=your-tos-bucket-name
TOS_ACCESS_KEY=your-tos-access-key
TOS_SECRET_KEY=your-tos-secret-key
TOS_ENDPOINT=https://tos-cn-beijing.volces.com
TOS_REGION=cn-beijing

# Optional: TOS object key path (default: uploads/test.png)
TOS_OBJECT_KEY=uploads/test.png

# ===== S3-Compatible OSS Configuration (for aws-s3.py) =====
# Required for S3-compatible OSS upload
OSS_BUCKET=your-oss-bucket-name

# Option 1: Use OSS-specific credentials
OSS_ACCESS_KEY_ID=your-oss-access-key-id
OSS_SECRET_ACCESS_KEY=your-oss-secret-access-key

# Option 2: Use AWS standard credential names (fallback)
# AWS_ACCESS_KEY_ID=your-aws-access-key-id
# AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key

# Optional: OSS endpoint URL (leave empty for AWS S3)
OSS_ENDPOINT=https://your-oss-endpoint.com

# Optional: Region (default: us-east-1)
OSS_REGION=us-east-1
# AWS_REGION=us-east-1

# Optional: OSS object key path (default: uploads/test.png)
OSS_OBJECT_KEY=uploads/test.png
