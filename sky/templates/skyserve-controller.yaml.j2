resources:
  cloud: gcp
  disk_size: 100
  ports:
{%- for port in ports %}
    - {{port}}
{%- endfor %}

{% if workdir is not none %}
workdir: {{workdir}}
{% endif %}

file_mounts:
  {{remote_task_yaml_path}}: {{local_task_yaml_path}}

envs:
{% if is_dev %}
  SKYPILOT_DEV: 1
{% endif %}
{% if is_debug %}
  SKYPILOT_DEBUG: 1
{% endif %}
{% if disable_logging %}
  SKYPILOT_DISABLE_USAGE_COLLECTION: 1
{% endif %}
